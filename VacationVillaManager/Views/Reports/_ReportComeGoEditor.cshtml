@model VacationVillaManager.Models.ReportModels.ReportComeGoModel

<div class="reportPane">
    <table>
        <tr>
            <td>@Html.LabelFor(model => model.Month)</td>
            <td>@Html.TextBoxFor(model => model.Month, new { @class = "month", @Value = "" })</td>
        </tr>
        <tr>
            <td>@Html.LabelFor(model => model.Email)</td>
            <td>@Html.TextBoxFor(model => model.Email, new { @class = "email", @Value = "", placeholder = "If blank no email will send" })</td>
        </tr>
    </table>

    <button class="generate">Generate!</button>
</div>